<script>
  // DÃ©tection automatique du pays par IP
  const countryCodeSelect = document.getElementById('countryCode');
  
  // Mapping des codes pays ISO vers les indicatifs tÃ©lÃ©phoniques
  const countryToCode = {
    'FR': '+33', 'US': '+1', 'CA': '+1', 'BE': '+32', 'CH': '+41', 'GB': '+44',
    'DE': '+49', 'ES': '+34', 'IT': '+39', 'NL': '+31', 'PT': '+351', 'MA': '+212',
    'SN': '+221', 'CI': '+225', 'BJ': '+229', 'BF': '+226', 'NE': '+227', 'TG': '+228',
    'MU': '+230', 'GH': '+233', 'NG': '+234', 'CM': '+237', 'CD': '+243', 'KE': '+254',
    'ZA': '+27', 'GR': '+30', 'LU': '+352', 'IE': '+353', 'IS': '+354', 'FI': '+358',
    'BG': '+359', 'HU': '+36', 'LT': '+370', 'LV': '+371', 'EE': '+372', 'UA': '+380',
    'HR': '+385', 'SI': '+386', 'CZ': '+420', 'SK': '+421', 'RU': '+7', 'JP': '+81',
    'KR': '+82', 'VN': '+84', 'CN': '+86', 'TR': '+90', 'IN': '+91', 'PK': '+92',
    'EG': '+20', 'DZ': '+213', 'TN': '+216', 'LY': '+218', 'GM': '+220', 'MR': '+222',
    'ML': '+223', 'GN': '+224', 'TD': '+235', 'CF': '+236', 'CV': '+238', 'ST': '+239',
    'GQ': '+240', 'GA': '+241', 'CG': '+242', 'AO': '+244', 'GW': '+245', 'SC': '+248',
    'SD': '+249', 'RW': '+250', 'ET': '+251', 'SO': '+252', 'DJ': '+253', 'TZ': '+255',
    'UG': '+256', 'BI': '+257', 'MZ': '+258', 'ZM': '+260', 'MG': '+261', 'RE': '+262',
    'ZW': '+263', 'NA': '+264', 'MW': '+265', 'LS': '+266', 'BW': '+267', 'SZ': '+268',
    'KM': '+269'
  };

  // Fonction pour dÃ©tecter le pays et prÃ©-sÃ©lectionner l'indicatif
  async function detectCountryAndSetCode() {
    try {
      // Utilisation d'une API gratuite pour dÃ©tecter le pays
      const response = await fetch('https://ipapi.co/json/');
      const data = await response.json();
      
      if (data.country_code && countryToCode[data.country_code]) {
        const phoneCode = countryToCode[data.country_code];
        countryCodeSelect.value = phoneCode;
      }
    } catch (error) {
      // En cas d'erreur, on garde la valeur par dÃ©faut (France)
      console.log('DÃ©tection du pays non disponible, utilisation de la valeur par dÃ©faut');
    }
  }

  // DÃ©tecter le pays au chargement de la page
  if (countryCodeSelect) {
    detectCountryAndSetCode();
  }

  const form = document.getElementById('inscriptionForm');
  const messageDiv = document.getElementById('formMessage');

  form.addEventListener('submit', async (e) => {
    e.preventDefault();
    
    const formData = new FormData(form);
    const countryCode = formData.get('countryCode') || '+33';
    const telephone = formData.get('telephone');
    
    const data = {
      prenom: formData.get('prenom'),
      nom: formData.get('nom'),
      email: formData.get('email'),
      telephone: telephone,
      countryCode: countryCode
      // Le groupId sera gÃ©rÃ© par la fonction Netlify via la variable d'environnement
    };

    // RÃ©initialiser le message
    messageDiv.className = 'form-message';
    messageDiv.textContent = '';

    // DÃ©sactiver le bouton pendant l'envoi
    const submitButton = form.querySelector('.submit-button');
    const originalButtonText = submitButton.querySelector('span').textContent;
    submitButton.disabled = true;
    submitButton.querySelector('span').textContent = 'Envoi en cours...';

    try {
      // VÃ©rifier si on est en local ou en production
      const isLocal = window.location.hostname === 'localhost' || window.location.hostname === '127.0.0.1';
      
      if (isLocal) {
        // Simulation en local pour le dÃ©veloppement
        await new Promise(resolve => setTimeout(resolve, 1500));
        console.log('ğŸ§ª Mode local : Simulation d\'inscription', data);
        // Rediriger vers la page de succÃ¨s mÃªme en local
        window.location.href = '/court-circuit-success';
        return;
      }

      // Appel rÃ©el Ã  la Netlify Function pour MailerLite
      const response = await fetch('/.netlify/functions/subscribe', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify(data)
      });

      const result = await response.json();

      // RÃ©activer le bouton
      submitButton.disabled = false;
      submitButton.querySelector('span').textContent = originalButtonText;

      if (response.ok && result.success) {
        // Rediriger vers la page de succÃ¨s
        window.location.href = '/court-circuit-success';
      } else {
        messageDiv.textContent = result.error || 'Une erreur est survenue. Veuillez rÃ©essayer.';
        messageDiv.classList.add('error');
      }
      
      // Masquer le message aprÃ¨s 5 secondes
      setTimeout(() => {
        messageDiv.className = 'form-message';
        messageDiv.textContent = '';
      }, 5000);
      
    } catch (error) {
      console.error('Erreur lors de l\'inscription:', error);
      messageDiv.textContent = 'Une erreur est survenue. Veuillez rÃ©essayer.';
      messageDiv.classList.add('error');
      submitButton.disabled = false;
      submitButton.querySelector('span').textContent = originalButtonText;
    }
  });
</script>

